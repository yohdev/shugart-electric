  "templateParts": [
    {
      "area": "header",
      "name": "Header"
    },
    {
      "area": "footer",
      "name": "Footer"
    }
  ],
  "version": 2,
  "$schema": "https://schemas.wp.org/wp/6.5/theme.json"
