  "styles": {
    "blocks": {
      "core/button": {
        "border": {
          "color": "var(--wp--preset--color--burrows-slate)",
          "radius": "3px",
          "style": "solid",
          "width": "1px"
        },
        "color": {
          "background": "var(--wp--preset--color--burrows-slate)",
          "text": "var(--wp--preset--color--white)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--white)"
            },
            "typography": {
              "fontSize": "12px !important"
            }
          }
        },
        "spacing": {
          "padding": {
            "bottom": "8px",
            "left": "22px",
            "right": "22px",
            "top": "8px"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontSize": "var(--wp--preset--font-size--normal)"
        },
        "variations": {
          "outline": {
            "border": {
              "color": "var(--wp--preset--color--white)",
              "radius": "3px",
              "style": "solid",
              "width": "1px"
            },
            "color": {
              "background": "transparent",
              "text": "var(--wp--preset--color--white)"
            }
          }
        }
      },
      "core/navigation": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--nunito)"
        }
      },
      "core/paragraph": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--segoe)",
          "fontSize": "var(--wp--preset--font-size--normal)",
          "lineHeight": "1.5"
        }
      },
      "acf/bc-menu": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontSize": "16px",
          "lineHeight": "26px"
        }
      }
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontWeight": "500",
          "lineHeight": "1.16"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "400",
          "lineHeight": "1.6"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "400",
          "lineHeight": "1.5"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--playfair)",
          "fontSize": "var(--wp--preset--font-size--normal-larger)",
          "fontWeight": "600",
          "lineHeight": "1.5"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--segoe)",
          "fontSize": "var(--wp--preset--font-size--normal)",
          "fontWeight": "600",
          "lineHeight": "1.5"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--playfair)",
      "fontSize": "var(--wp--preset--font-size--normal)"
    }
